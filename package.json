{
  "name": "create-codes",
  "version": "0.0.0",
  "scripts": {
    "test:cli": "turbo run build --filter=create-codes test --filter=create-codes",
    "test:modules": "turbo run test --filter='./examples/*'",
    "e2e": "turbo run e2e && turbo run e2e:mini",
    "type": "turbo run type --filter=!reactjs-boilerplate",
    "lint": "turbo run lint --filter=!reactjs-boilerplate",
    "prepare": "husky install"
  },
  "devDependencies": {
    "turbo": "1.7.0",
    "prettier": "2.8.3",
    "husky": "8.0.3",
    "lint-staged": "13.1.0",
    "eslint-config-custom": "workspace:*"
  },
  "lint-staged": {
    "*": "prettier --ignore-unknown --write"
  }
}
